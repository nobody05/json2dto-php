{
    "name": "phpone/json2dto",
    "license": "GPL-3.0",
    "description": "Convert JSON files to Spatie DTO objects",
    "keywords": ["spatie", "laravel", "dto", "converter", "json"],
    "homepage": "https://json2dto.atymic.dev",
    "type": "project",
    "support": {
        "source": "https://github.com/nobody05/json2dto-php"
    },
    "authors": [
        {
            "name": "phpone",
            "email": "1035948193@qq.com",
            "homepage": "https://atymic.dev"
        }
    ],
    "require": {
        "php": "^7.4",
        "ext-json": "*",
        "illuminate/support": "^8.0",
        "nette/php-generator": "^3.3",
        "spatie/data-transfer-object": "^1.13  || ^2.0",
        "symfony/console": "^4.2"

    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^9",
        "spatie/phpunit-snapshot-assertions": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Atymic\\Json2Dto\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "scripts": {
        "test:ci": "phpunit --verbose --coverage-clover=coverage.xml"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "bin": ["json2dto"]
}
